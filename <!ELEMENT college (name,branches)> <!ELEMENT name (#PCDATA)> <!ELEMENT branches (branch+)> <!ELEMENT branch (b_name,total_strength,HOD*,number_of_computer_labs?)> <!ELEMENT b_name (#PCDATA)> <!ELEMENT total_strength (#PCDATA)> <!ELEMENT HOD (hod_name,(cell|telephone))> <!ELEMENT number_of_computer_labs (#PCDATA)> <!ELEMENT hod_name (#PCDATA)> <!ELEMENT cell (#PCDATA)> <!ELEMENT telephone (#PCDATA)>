<!ELEMENT college (name,branches)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT branches (branch+)>
<!ELEMENT branch (b_name,total_strength,HOD*,number_of_computer_labs?)>
<!ELEMENT b_name (#PCDATA)>
<!ELEMENT total_strength (#PCDATA)>
<!ELEMENT HOD (hod_name,(cell|telephone))>
<!ELEMENT number_of_computer_labs (#PCDATA)>
<!ELEMENT hod_name (#PCDATA)>
<!ELEMENT cell (#PCDATA)>
<!ELEMENT telephone (#PCDATA)>


